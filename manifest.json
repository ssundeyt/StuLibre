{
  "name": "StuLibre",
  "version": "0.8.5",
  "description": "Thou shall not force students to PAY for open-source notes & classwork. Notes that other students have shared is not yours to capitalise on, Studocu.",
  "icons": {
    "16": "img/icon16.png",
    "48": "img/icon48.png",
    "128": "img/icon128.png"
  },
  "content_scripts": [
    {
      "js": ["version-check.js", "remove-banner.js", "remove-blur.js", "doc-viewer.js"],
      "css": ["style.css"],
      "matches": ["https://www.studocu.com/*"]
    }
  ],
  "permissions": [
    "activeTab",
    "tabs"
  ],
  "host_permissions": [
    "<all_urls>"
  ],
  "manifest_version": 3
}